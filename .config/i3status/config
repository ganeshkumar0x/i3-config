general {
    colors = false
    interval = 1
}

order += "wireless _first_"
order += "volume master"
order += "battery 0"
order += "time"

wireless _first_ {
    format_up = "W:%essid"
    format_down = "W:down"
}

volume master {
    format = "V:%volume"
    format_muted = "V:muted (%volume)"
}

battery 0 {
    format = "B:%percentage %status"
    integer_battery_capacity = true
    last_full_capacity = true
}

time {
    format = "%A, %B %-d at %H:%M "
}
